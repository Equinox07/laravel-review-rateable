{
    "name": "equinox07/laravel-review-rateable",
    "description": "Review & Rating system for Laravel 7, 8 & 9",
    "keywords": ["rating", "Ratable", "laravel", "Review-Rateable", "reviewable"],
    "license": "MIT",
    "authors": [
        {
            "name": "CodebyRay",
            "email": "dev@codebyray.com"
        }
    ],
    "minimum-stability": "dev",
    "repositories": [
        {
            "type": "vcs",
            "url": "https://github.com/Equinox07/laravel-review-rateable"
        }
    ],
    "require": {
        "php" : "^7.4|^8.0",
        "illuminate/support": "^7.0|^8.0|^9.0|^10.0",
        "illuminate/database": "^7.0|^8.0|^9.0|^10.0"
    },
    "require-dev": {
        "phpunit/phpunit": "^6.3|^7.0|^8.0|^9.0|^9.5",
        "orchestra/testbench": "~3.5.0|~3.6.0|^4.0|^5.0|^6.0|^7.0"
    },
    "autoload": {
        "psr-4": {
            "Codebyray\\ReviewRateable\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Codebyray\\ReviewRateable\\Test\\": "tests"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "Codebyray\\ReviewRateable\\ReviewRateableServiceProvider"
            ]
        }
    },
    "scripts": {
        "test": "vendor/bin/phpunit",
        "test:windows": "vendor\\bin\\phpunit"
    }
}
